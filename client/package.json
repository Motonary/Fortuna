{
  "name": "fortuna",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Motonary",
  "license": "ISC",
  "scripts": {
    "release": "webpack --mode production --config webpack.config.js",
    "watch": "webpack-dev-server --mode development --history-api-fallback",
    "storybook": "start-storybook -p 9001 -c src/.storybook",
    "lint": "yarn tslint ./src/**/*.{ts,tsx}",
    "fix": "yarn tslint --fix ./src/javascripts/**/*.{ts,tsx}",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/classnames": "2.2.7",
    "@types/keymirror": "0.1.1",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.21",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "7.0.1",
    "@types/react-router-dom": "4.3.1",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "formik": "1.4.3",
    "keymirror": "0.1.1",
    "lodash": "4.17.11",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@storybook/addon-storysource": "4.1.11",
    "@storybook/addons": "4.1.11",
    "@storybook/react": "4.1.11",
    "@types/enzyme": "3.1.16",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.13",
    "@types/storybook__react": "4.0.0",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "file-loader": "3.0.1",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.0.0",
    "jest-enzyme": "7.0.1",
    "lint-staged": "8.1.3",
    "mini-css-extract-plugin": "0.5.0",
    "prettier": "1.16.3",
    "react-test-renderer": "16.7.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-config-standard": "8.0.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "engines": {
    "yarn": "1.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>src/__mocks__/fileTransformer.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "!/node_modules/types"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "verbose": true
  }
}
